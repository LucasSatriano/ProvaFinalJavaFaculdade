<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="711.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EFAXALUNO.FXMLControleAluno">
   <children>
      <Button fx:id="BTNCADASTRAR" layoutX="290.0" layoutY="82.0" mnemonicParsing="false" onAction="#cadastrar" text="Cadastrar" />
